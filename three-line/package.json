{
  "name": "@GW.DDJJCS/three-line",
  "version": "0.0.12",
  "dependencies": {
    "bcore": "0.0.18",
    "echarts": "^4.7.0",
    "jquery": "2.1.4",
    "lodash": "4.6.1"
  },
  "datav": {
    "cn_name": "three-line",
    "icon": "//datav.oss-cn-hangzhou.aliyuncs.com/uploads/images/2c1d2528903dfa80139748951f376445.png",
    "protocol": 2,
    "type": [
      "regular"
    ],
    "view": {
      "width": "400",
      "height": "200",
      "minWidth": "200",
      "minHeight": "100"
    },
    "apis": {
      "source": {
        "handler": "render",
        "description": "接口描述",
        "fields": {
        }
      }
    },
    "config": {
      "grid": {
        "name": "图表间距",
        "type": "group",
        "fold": true,
        "children": {
          "top": {
            "name": "顶部",
            "type": "text",
            "default": "15%"
          },
          "right": {
            "name": "右侧",
            "type": "text",
            "default": "5%"
          },
          "bottom": {
            "name": "底部",
            "type": "text",
            "default": "5%"
          },
          "left": {
            "name": "左侧",
            "type": "text",
            "default": "5%"
          }
        }
      },
      "xAxis": {
        "name": "x轴",
        "type": "group",
        "children": {
          "axisLabel": {
            "name": "刻度标签",
            "type": "group",
            "children": {
              "textStyle": {
                "name": "文本样式",
                "type": "group",
                "children": {
                  "color": {
                    "name": "颜色",
                    "type": "color",
                    "default": "#fff"
                  },
                  "fontWeight": {
                    "name": "字体粗细",
                    "type": "text",
                    "default": "bold"
                  },
                  "fontSize": {
                    "name": "字号",
                    "type": "number",
                    "default": 20
                  }
                },
                "fold": true
              },
              "margin": {
                "name": "数字边距",
                "type": "number",
                "default": 28
              }
            }
          },
          "axisLine": {
            "name": "轴线",
            "type": "group",
            "children": {
              "lineStyle": {
                "name": "轴线样式",
                "type": "group",
                "children": {
                  "width": {
                    "name": "宽度",
                    "type": "number",
                    "default": 2
                  }
                }
              }
            }
          }
        }
      },
      "yAxis": {
        "name": "y轴",
        "type": "group",
        "children": {
          "nameTextStyle": {
            "name": "名称样式",
            "type": "group",
            "children": {
              "color": {
                "name": "颜色",
                "type": "color",
                "default": "#fff"
              },
              "fontWeight": {
                "name": "字体粗细",
                "type": "text",
                "default": "bold"
              },
              "fontFamily": {
                "name": "字体",
                "type": "text",
                "default": "sans-serif"
              },
              "fontSize": {
                "name": "字号",
                "type": "number",
                "default": 20
              }
            },
            "fold": true
          },
          "axisLabel": {
            "name": "刻度标签",
            "type": "group",
            "children": {
              "textStyle": {
                "name": "文本样式",
                "type": "group",
                "children": {
                  "color": {
                    "name": "颜色",
                    "type": "color",
                    "default": "#fff"
                  },
                  "fontWeight": {
                    "name": "字体粗细",
                    "type": "text",
                    "default": "normal"
                  },
                  "fontSize": {
                    "name": "字号",
                    "type": "number",
                    "default": 25
                  }
                },
                "fold": true
              },
              "margin": {
                "name": "数字边距",
                "type": "number",
                "default": 25
              }
            }
          },
          "axisLine": {
            "name": "轴线",
            "type": "group",
            "children": {
              "lineStyle": {
                "name": "轴线样式",
                "type": "group",
                "children": {
                  "width": {
                    "name": "宽度",
                    "type": "number",
                    "default": 2
                  }
                }
              }
            }
          }
        }
      },
      "dot1": {
        "name": "圆点1",
        "type": "image",
        "default": "//datav.oss-cn-hangzhou.aliyuncs.com/uploads/images/1c8b76f2431160345363747062906554.png"
      },
      "dot2": {
        "name": "圆点2",
        "type": "image",
        "default": "//datav.oss-cn-hangzhou.aliyuncs.com/uploads/images/ded2919e8e1107d75f207bd3e63fd361.png"
      }
